<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Address" table="address">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="street" type="string" column="street" length="90"/>
    <field name="settlement" type="string" column="settlement" length="90"/>
    <field name="city" type="string" column="city" length="90"/>
    <field name="state" type="string" column="state" length="45"/>
    <field name="zipCode" type="string" column="zip_code" length="5"/>
    <field name="lat" type="float" column="lat"/>
    <field name="long" type="float" column="long"/>
  </entity>
</doctrine-mapping>
